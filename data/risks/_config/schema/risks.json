{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "risks.json",
  "title": "Risk Framework",
  "description": "Risk framework scores for Yearn strategies",
  "type": "object",
  "properties": {
    "label": {
      "type": "string"
    },
    "codeReviewScore": {
      "type": "number"
    },
    "testingScore": {
      "type": "number"
    },
    "auditScore": {
      "type": "number"
    },
    "protocolSafetyScore": {
      "type": "number"
    },
    "complexityScore": {
      "type": "number"
    },
    "teamKnowledgeScore": {
      "type": "number"
    },
    "criteria": {
      "type": "object",
      "required": ["strategies"],
      "properties": {
        "strategies": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "address"
          }
        }
      }
    }
  },
  "required": ["label", "criteria", "codeReviewScore", "testingScore", "auditScore", "protocolSafetyScore", "complexityScore", "teamKnowledgeScore"]
}
